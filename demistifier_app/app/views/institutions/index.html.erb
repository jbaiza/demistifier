<p id="notice"><%= notice %></p>

<h1>Institutions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Alternate names</th>
      <th>Reg nr</th>
      <th>Lr izm code</th>
      <th>Address</th>
      <th>Institution Type</th>
      <th>Email</th>
      <th>Url</th>
      <th>Lat</th>
      <th>Lon</th>
      <th>Institution id source</th>
      <th>Region</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @institutions.each do |institution| %>
      <tr>
        <td><%= institution.name %></td>
        <td><%= institution.alternate_names %></td>
        <td><%= institution.reg_nr %></td>
        <td><%= institution.lr_izm_code %></td>
        <td><%= institution.address %></td>
        <td><%= institution.institution_type %></td>
        <td><%= institution.email %></td>
        <td><%= institution.url %></td>
        <td><%= institution.lat %></td>
        <td><%= institution.lon %></td>
        <td><%= institution.institution_id_source %></td>
        <td><%= institution.region %></td>
<% if false %>
        <td><%= link_to 'Show', institution %></td>
        <td><%= link_to 'Edit', edit_institution_path(institution) %></td>
        <td><%= link_to 'Destroy', institution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
<% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if false %>
<%= link_to 'New Institution', new_institution_path %>
<% end %>
